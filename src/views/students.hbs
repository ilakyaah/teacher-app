<div class="container">
  <h1 class="text-center">STUDENTS DETAILS</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Student ID</th>
        <th scope="col">studentName</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      {{#each students}}
      <tr>
        <td>{{this.studentID}}</td>
        <td>{{this.studentName}}</td>

        <td><a href="/teacher/{{../teacher}}/edit-student/{{this.studentID}}">Edit</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  <a class="btn btn-primary btn-lg" href="/teacher/{{teacher}}/add-student">Add Student</a>
</div>